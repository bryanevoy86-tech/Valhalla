# --- VALHALLA SANDBOX (PHASE 3) ---
VALHALLA_ENV=sandbox

# KEEP DRY RUN ON (DO NOT CHANGE)
VALHALLA_DRY_RUN=1

# Turn on real-data ingestion ONLY (no outbound)
VALHALLA_PHASE=3
VALHALLA_REAL_DATA_INGEST=1

# Where Phase 3 reads from
VALHALLA_REAL_LEADS_DIR=data/inbox/real_leads

# Hard caps (safety)
VALHALLA_MAX_LEADS_PER_CYCLE=25
VALHALLA_MAX_ACTIONS_PER_CYCLE=0

# Explicitly disable outbound (if your system supports this flag)
VALHALLA_DISABLE_OUTBOUND=1
